{
  "name": "ngcomponent",
  "version": "2.0.4",
  "description": "A clean React-like abstraction for rendering non-Angular components within an Angular app.",
  "main": "index.js",
  "main:esnext": "index.es2015.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "rm index.d.ts ; tsc -d -m es2015 --outDir . && mv ./index.js ./index.es2015.js && tsc -m commonjs -t es5  --outDir .",
    "tdd": "npm run watch & karma start",
    "test": "npm run build && karma start --single-run",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/coatue-oss/ngcomponent.git"
  },
  "keywords": [
    "angular",
    "component",
    "react",
    "directive",
    "props",
    "state",
    "controller"
  ],
  "author": "Boris Cherny <boris@performancejs.com>",
  "contributors": [
    "Chris Khoo <chris.khoo@gmail.com>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/coatue-oss/ngcomponent/issues"
  },
  "homepage": "https://github.com/coatue-oss/ngcomponent#readme",
  "devDependencies": {
    "@types/angular-mocks": "^1.5.9",
    "@types/jasmine": "^2.5.46",
    "browserify": "^14.0.1",
    "jasmine": "^2.5.3",
    "karma": "^1.5.0",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-source-map-support": "^1.2.0",
    "ngimport": "^0.6.0",
    "rollupify": "^0.3.9",
    "tslint": "^4.5.1",
    "tslint-no-circular-imports": "^0.1.0",
    "typescript": "^2.2.1",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "@types/angular": "^1.6.9",
    "@types/lodash": "^4.14.55",
    "angular": "^1.6.3",
    "lodash": "^4.17.4"
  }
}
